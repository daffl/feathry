{
  "bitbucket": {
    "name": "bitbucket",
    "strategy": "passport-bitbucket-oauth2",
    "tokenStrategy": null,
    "permissions": {
      "scope": ["account"]
    }
  },
  "dropbox": {
    "name": "dropbox",
    "strategy": "passport-dropbox-oauth2",
    "tokenStrategy": null,
    "permissions": {
      "scope": []
    }
  },
  "facebook": {
    "name": "facebook",
    "strategy": "passport-facebook",
    "tokenStrategy": "passport-facebook-token",
    "permissions": {
      "scope": ["public_profile", "email"]
    }
  },
  "github": {
    "name": "github",
    "strategy": "passport-github",
    "tokenStrategy": "passport-github-token",
    "permissions": {
      "scope": []
    }
  },
  "google": {
    "name": "google",
    "strategy": "passport-google-oauth20",
    "tokenStrategy": "passport-google-token",
    "permissions": {
      "scope": ["profile"]
    }
  },
  "instagram": {
    "name": "instagram",
    "strategy": "passport-instagram",
    "tokenStrategy": "passport-instagram-token",
    "permissions": {
      "scope": ["basic"]
    }
  },
  "linkedin": {
    "name": "linkedin",
    "strategy": "passport-linkedin-oauth2",
    "tokenStrategy": "passport-linkedin-token-oauth2",
    "permissions": {
      "scope": ["r_emailaddress", "r_basicprofile"]
    }
  },
  "local": {
    "name": "local"
  },
  "paypal": {
    "name": "paypal",
    "strategy": "passport-paypal-oauth",
    "tokenStrategy": "passport-paypal-token",
    "permissions": {
      "scope": ["https://identity.x.com/xidentity/resources/profile/me"]
    }
  },
  "spotify": {
    "name": "spotify",
    "strategy": "passport-spotify",
    "tokenStrategy": null,
    "permissions": {
      "scope": ["user-read-email", "user-read-private"]
    }
  }
}