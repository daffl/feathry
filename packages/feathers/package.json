{
  "name": "@feathersjs/feathers",
  "description": "A framework for real-time applications and REST API with JavaScript and TypeScript",
  "version": "4.5.4",
  "homepage": "http://feathersjs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/feathersjs/feathers.git"
  },
  "keywords": [
    "feathers",
    "REST",
    "socket.io",
    "realtime"
  ],
  "main": "lib/index.js",
  "types": "index.d.ts",
  "author": {
    "name": "Feathers contributors",
    "email": "hello@feathersjs.com",
    "url": "https://feathersjs.com"
  },
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/feathers"
  },
  "bugs": {
    "url": "https://github.com/feathersjs/feathers/issues"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "write-version": "node -e \"console.log('export default \\'' + require('./package.json').version + '\\';')\" > lib/version.js",
    "reset-version": "node -e \"console.log('export default \\'development\\';')\" > lib/version.js",
    "version": "npm run write-version && npm run compile",
    "publish": "npm run reset-version",
    "compile": "shx rm -rf lib/ && tsc",
    "test": "npm run compile && mocha --config ../../.mocharc.ts.json --recursive test/**.test.ts test/**/*.test.ts"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@feathersjs/commons": "^4.5.4",
    "@feathersjs/hooks": "^0.4.0-alpha.0",
    "debug": "^4.1.1",
    "events": "^3.1.0",
    "uberproto": "^2.0.6"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "mocha": "^7.1.2",
    "shx": "^0.3.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "gitHead": "9b9f0f13387341bdd320f1e66feda828fca2c9f2"
}
